default  partial alphanumeric_keys modifier_keys

partial alphanumeric_keys
xkb_symbols "xzorak1" {

    name[Group1]= "English (Xzorak, transitional Dvorak to Colemak layout)";

    include "us(dvorak)"

        // Top Row
    key <AE11> { [	minus,	underscore	]	};
    key <AE12> { [	equal,	plus		]	};

    // Upper Row
    key <AD11> { [  bracketleft, braceleft	]	};
    key <AD12> { [ bracketright, braceright,  dead_tilde] };
    key <BKSP> { [       Return,  Return        ] };

    // Home Row
    key <CAPS> { [   BackSpace, Escape         ] };
    key <AC11> { [   semicolon,	colon           ] };

    // Lower Row
    key <AB01> { [	    x,	X		]	};
    key <AB05> { [	    z,	Z		]	};

    key <AB10> { [	slash,	question	]	};

};

xkb_symbols "xzorak2" {

    name[Group1]= "English (Xzorak, transitional Dvorak to Colemak layout)";

    include "xzorak(xzorak1)"

    // Upper Row
    key <AD01> { [	    q,	Q         	]	};
    key <AD07> { [      l,  L       	]	};
    key <AD08> { [      u,  U           ]   };
    key <AD10> { [ semicolon,  colon    ]   };

    // Home Row
    key <AC04> { [	    g,	G		    ]	};
    key <AC11> {	[ apostrophe,	quotedbl	]	};

    // Lower Row
    key <AB02> { [	    c,	C		    ]	};

};

